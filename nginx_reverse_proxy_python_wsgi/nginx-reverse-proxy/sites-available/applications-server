server {
    listen 85;
    server_name nginx_reverse_proxy_python_wsgi nginx_reverse_proxy_python_wsgi.local;
    include proxy.conf;

    location /app1/ {
        proxy_pass http://192.168.0.204:80/app1/api/welcome/;
    }

    location /app2/ {
        proxy_pass http://192.168.0.204:80/app2/api/welcome/;
    }
}

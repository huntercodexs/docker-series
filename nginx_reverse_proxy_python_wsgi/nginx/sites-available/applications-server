server {
    listen 85;
    server_name nginx_reverse_proxy_python nginx_reverse_proxy_python.local;

    location /app1/ {
        include proxy_params;
        #proxy_pass http://unix:/home/python3/applications/app1/app1.sock;
        proxy_pass http://127.0.0.1:55001/api/welcome;
    }

    location /app2/ {
        include proxy_params;
        #proxy_pass http://unix:/home/python3/applications/app2/app2.sock;
        proxy_pass http://127.0.0.1:55002/api/welcome;
    }
}
